{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension allows the user to change the background color",
    "version": "1.0",

    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "src/popup.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}
